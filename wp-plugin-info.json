{
   "id": "WP_Plugin_Info",
   "name": "WP Plugin Info",
   "version": "1.0.5",
   "description": {
      "en": "Get and save plugin information from WordPress API to be consumed by other modules",
      "es": "Obtener y guardar información de plugin alojados en el repositorio de WordPress a través de su API"
   },
   "state": "active",
   "category": "eliasis-wp-plugin",
   "url": "https://github.com/eliasis-framework/wp-plugin-info.git",
   "author": "Josantonius",
   "author-url": "https://josantonius.com/",
   "license": "MIT",
   "hooks-controller": "Main",
   "url-import": "https://raw.githubusercontent.com/eliasis-framework/wp-plugin-info/master/",
   "installation-files": {
      "wp-plugin-info": {
         "0": "wp-plugin-info.json",
         "1": "LICENSE",
         "2": "composer.json",
         "config": [
            "add-paths.php",
            "add-urls.php",
            "files.php",
            "namespaces.php",
            "settings.php"
         ],
         "public": {
            "images": [
               "wp-plugin-info.png"
            ]
         },
         "src": {
            "controller": [
               "class-main.php"
            ],
            "model": [
               "class-main.php"
            ],
            "data": [
               "plugins.json"
            ]
         }
      }
   }
}